{
  "self": {
    "layout": "default",
    "logo": {
      "default": "media/svg/logos/logo-title-producto.svg",
      "sticky": "media/svg/logos/logo-title-producto.svg"
    }
  },
  "loader": {
    "type": "spinner-logo",
    "logo": "media/logos/adv-isotipo.png",
    "message": "Por favor espera..."
  },
  "breakpoints": {
    "sm": 576,
    "md": 768,
    "lg": 992,
    "xl": 1200,
    "xxl": 1200
  },
  "colors": {
    "theme": {
      "base": {
        "white": "#ffffff",
        "primary": "#074A7B",
        "secondary": "#E5EAEE",
        "success": "#1BC5BD",
        "highlight": "#378EF0",
        "info": "#8950FC",
        "warning": "#FFA800",
        "danger": "#F64E60",
        "light": "#F3F6F9",
        "dark": "#212121"
      }
    },
    "gray": {
      "gray-100": "#F3F6F9",
      "gray-200": "#ECF0F3",
      "gray-300": "#E5EAEE",
      "gray-400": "#D6D6E0",
      "gray-500": "#B5B5C3",
      "gray-600": "#80808F",
      "gray-700": "#464E5F",
      "gray-800": "#1B283F",
      "gray-900": "#212121"
    }
  },
  "font-family": "Source Sans Pro",
  "header": {
    "self": {
      "width": "fixed",
      "fixed": {
        "desktop": true,
        "mobile": true
      }
    },
    "menu": {
      "self": {
        "display": false,
        "layout": "default",
        "root-arrow": false
      },
      "desktop": {
        "arrow": true,
        "toggle": "click",
        "submenu": {
          "theme": "light",
          "arrow": true
        }
      },
      "mobile": {
        "submenu": {
          "theme": "dark",
          "accordion": true
        }
      }
    }
  },
  "subheader": {
    "display": true,
    "displayDesc": false,
    "displayDaterangepicker": true,
    "breadcrumb": {
      "display": true
    },
    "layout": "subheader-v6",
    "fixed": false,
    "width": "fixed",
    "clear": false,
    "style": "transparent"
  },
  "content": {
    "width": "fixed"
  },
  "aside": {
    "self": {
      "display": false
    },
    "menu": {
      "dropdown": false,
      "scroll": false,
      "submenu": {
        "accordion": true,
        "dropdown": {
          "arrow": true,
          "hover-timeout": 500
        }
      }
    }
  },
  "footer": {
    "width": "fixed",
    "layout": "compact"
  }
}
